{
  "name": "shurper/yii2-editmodal",
  "description": "Yii2 to update models in bootstrap modals",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "keywords": [
    "shurper",
    "yii",
    "yii2",
    "yii 2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "shurper",
      "email": "shurper@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.9",
    "yiisoft/yii2-jui": "*",
    "floor12/yii2-notification": "*"
  },
  "autoload": {
    "psr-4": {
      "shurper\\editmodal\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-editmodal"
    }
  ]
}
